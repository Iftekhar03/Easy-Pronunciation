# cook your dish here
trials = int(input())
for i in range(trials):
    consonants = 0
    letters = input()
    for x in [*str(input())]:
        if x == 'a' or x == 'e' or x == 'i' or x == 'o' or x == 'u':
            consonants = 0
        else:
            consonants += 1
        if consonants == 4:
            break
    if consonants == 4:
        print('NO')
    else:
        print('YES')
